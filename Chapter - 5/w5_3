/*(Random strings) Write a program that generates a 
random string with six lowercase letters.*/

#include <iostream>
#include <time.h>

using namespace std;

char in;

int main()
{
    srand (time(NULL));
    cout << "Generating random string of 6 lower case letters..."<<endl;
    cout<<"The random string is: ";
    for(int i=1;i<=5;i++)
    {   cout<<char(rand() % 26 + 96);
    }




    return 0;
}
